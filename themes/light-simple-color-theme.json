{
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#000000",
    "editor.inactiveSelectionBackground": "#E5EBF1",
    "editorIndentGuide.background": "#D3D3D3",
    "editorIndentGuide.activeBackground": "#939393",
    "editor.selectionHighlightBackground": "#ADD6FF80",
    "editorSuggestWidget.background": "#F3F3F3",
    "activityBarBadge.background": "#007ACC",
    "sideBarTitle.foreground": "#6F6F6F",
    "list.hoverBackground": "#E8E8E8",
    "input.placeholderForeground": "#767676",
    "searchEditor.textInputBorder": "#CECECE",
    "settings.textInputBorder": "#CECECE",
    "settings.numberInputBorder": "#CECECE",
    "statusBarItem.remoteForeground": "#FFF",
    "statusBarItem.remoteBackground": "#16825D"
  },
  "semanticHighlighting": true,
  // blue: #0451a5
  // green: #219186
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#000000ff"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#93A1A1"
      }
    },
    {
      "scope": "constant.language", // Ruby: symbol, true, false, nil
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "entity.name.operator.custom-literal.number",
        "variable.other.enummember",
        "keyword.operator.plus.exponent",
        "keyword.operator.minus.exponent"
      ],
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "css tags in selectors, xml tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "entity.name.selector",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [ // CSS #id .class
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#cd3131"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "",
        "foreground": "#000080"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "",
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [ // markdown: list *
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "markup.inline.raw", // markdown: `...`
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "meta.preprocessor",  // C: #define NAME
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "meta.preprocessor.string", // don't know
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
      "scope": "meta.preprocessor.numeric", // don't know
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",  // don't know
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "storage.type", // JavaScript: var, let, const
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": [
        "storage.modifier",
        "keyword.operator.noexcept"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "string",
        "entity.name.operator.custom-literal.string",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "scope": [
        "string.comment.buffered.block.pug",
        "string.quoted.pug",
        "string.interpolated.pug",
        "string.unquoted.plain.in.yaml",
        "string.unquoted.plain.out.yaml",
        "string.unquoted.block.yaml",
        "string.quoted.single.yaml",
        "string.quoted.double.xml",
        "string.quoted.single.xml",
        "string.unquoted.cdata.xml",
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.unquoted.html",
        "string.quoted.single.handlebars",
        "string.quoted.double.handlebars"
      ],
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "String interpolation",
      "scope": [  // #{}, ${}
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "scope": [  // <% %>
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [ // CSS: value
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "scope": [ // CSS: property name
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [  // JSON: key in { "key": "value" }
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "keyword", // general keywords
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "keyword.other.special-method.ruby", // include, private, attr_accessor ...
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "keyword.control", // if, do, class, def, end, ...
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike"
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "name": "Variable start", // Ruby: instance variable's @
      "scope": "punctuation.definition.variable",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      // Ruby: symbols as hash key
      "scope": [
        "constant.other.symbol.hashkey.ruby",
        "constant.language.symbol.hashkey.ruby"
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    }
  ]
}
